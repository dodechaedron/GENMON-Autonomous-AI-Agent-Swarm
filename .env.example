# GENMON Environment Variables
# Copy this to .env and fill in your values
# NEVER commit .env to git!

# === SERVER-ONLY (not exposed to browser) ===

# Your wallet private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Discord webhook URL for launch notifications (optional)
DISCORD_WEBHOOK=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Twitter API Bearer Token for sentiment analysis (optional)
TWITTER_BEARER=your_twitter_bearer_token_here

# Telegram Bot (get from @BotFather on Telegram)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Supabase (server-side — service_role key)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# === CLIENT-SAFE (exposed to browser via NEXT_PUBLIC_) ===

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WC_PROJECT_ID=your_walletconnect_project_id_here

# Supabase (client-safe — anon key)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Nad.fun factory contract address (update when available)
NEXT_PUBLIC_NADFUN_FACTORY=

# PostgreSQL direct connection (for migration scripts)
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/postgres
